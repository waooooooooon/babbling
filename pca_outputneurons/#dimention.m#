pfunction [y,ruiseki,kiyo]= dimention(id)
y=0;


firings=importdata(id);

Firings=zeros(100,1000);

for i=1:1000
  
    I=firings(find(firings(:,2)==i),3); %t=iに発火したニューロンidをIに
    C=size(I);  %Iのサイズ
    for j=1:C(1,1)
    Firings(I(j,1),i)=1;
    end
    
end

Firings=Firings.';

%Firings=zscore(Firings);

[COEFF,SCORE,latent] = pca(Firings);

kiyo=latent/sum(latent);
ruiseki=zeros(size(kiyo));

for i=1:size(kiyo)
    if i==1
    ruiseki(1)=kiyo(i);
    
    else
        ruiseki(i)=ruiseki(i-1)+kiyo(i);
    end
    
end

y=min(find(ruiseki>0.8));



%size(kiyo)
%sum(kiyo)

